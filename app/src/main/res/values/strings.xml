<resources>
    <string name="app_name">MoodTracker</string>
    <string name="sad">Sad</string>
    <string name="disappointed">Disappointed</string>
    <string name="normal">Normal</string>
    <string name="happy">Happy</string>
    <string name="superHappy">Super Happy</string>
    <string name="alertDialogTitle">Comment</string>
    <string name="ok">OK</string>
    <string name="cancel">CANCEL</string>
    <string name="aWeekAgo">A week ago</string>
    <string name="numberDay">%1$d days ago</string>
    <string name="twoDayAgo">2 days ago</string>
    <string name="yesterday">Yesterday</string>
    <string name="today">Today</string>


