<resources>
    <string name="app_name">MoodTracker</string>

    <array name="smiley">
        <item>@drawable/smiley_sad</item>
        <item>@drawable/smiley_disappointed</item>
        <item>@drawable/smiley_normal</item>
        <item>@drawable/smiley_happy</item>
        <item>@drawable/smiley_super_happy</item>
    </array>

    <array name="color">
        <item>@color/faded_red</item>
        <item>@color/warm_grey</item>
        <item>@color/cornflower_blue_65</item>
        <item>@color/light_sage</item>
        <item>@color/banana_yellow</item>
    </array>

    <string-array name="description">
        <item>Sad</item>
        <item>Disappointed</item>
        <item>Normal</item>
        <item>Happy</item>
        <item>Super Happy</item>
    </string-array>

    <array name="sound">
        <item>@raw/sad</item>
        <item>@raw/disappointed</item>
        <item>@raw/normal</item>
        <item>@raw/happy</item>
        <item>@raw/superhappy</item>
    </array>

</resources>
